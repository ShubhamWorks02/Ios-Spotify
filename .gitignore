# Add any directories, files, or patterns you don't want to be tracked by version control

# Xcode
# Ignore Xcode project build directory
build/

*.generated.swift

# Ignore Node.js modules
node_modules/

# Ignore Xcode's Derived Data directory
DerivedData/

## Build generated files
# Ignore user-specific Xcode project settings
*.pbxuser
!default.pbxuser
*.mode1v3
!default.mode1v3
*.mode2v3
!default.mode2v3
*.perspectivev3
!default.perspectivev3

# Ignore Xcode user-specific workspace settings
xcuserdata/

## Other
# Ignore files moved aside by Xcode during migration
*.moved-aside

# Ignore Xcode's workspace metadata
*.xccheckout

# Ignore Xcode Source Control Blueprint
*.xcscmblueprint

# Ignore macOS system-specific file
.DS_Store

## Obj-C/Swift specific
# Ignore Obj-C header map file
*.hmap

# Ignore iOS app archive
*.ipa

# Ignore iOS app symbolication files
*.dSYM.zip
*.dSYM

## Playgrounds
# Ignore Xcode playground timeline data
timeline.xctimeline

# Ignore Xcode playground workspace
playground.xcworkspace

# Swift Package Manager
# Ignore Swift Package Manager build directory
.build/

# CocoaPods
# Ignore CocoaPods installed libraries
Pods/
/.DS_Store

# Carthage
# Ignore Carthage build directory
Carthage/Build

# fastlane
# Ignore fastlane generated report files
fastlane/report.xml
fastlane/Preview.html
fastlane/screenshots
fastlane/test_output

# Ignore any files that are not specified in the previous rules
*.

